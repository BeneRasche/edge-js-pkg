{
  "name": "edge-js-pkg",
  "version": "1.0.0",
  "main": "index.js",
  "bin": "index.js",
  "dependencies": {
    "edge-js": "^23.1.4",
    "pkg": "npm:@yao-pkg/pkg@^6.3.1"
  },
  "scripts": {
    "start": "node index.js",
    "pkg": "pkg index.js -c package.json --no-native-build --output Edge.exe"
  },
  "pkg": {
    "assets": "node_modules/edge-js/lib/native/win32",
    "targets": [
      "node20-win"
    ]
  }
}
