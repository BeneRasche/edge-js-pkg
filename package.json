{
  "name": "edge-js-pkg",
  "version": "1.0.0",
  "main": "index.js",
  "bin": "index.js",
  "dependencies": {
    "edge-js": "^23.1.4",
    "pkg": "^5.8.1"
  },
  "scripts": {
    "start": "node index.js",
